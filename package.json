{
  "name": "multi-app-workspace",
  "version": "0.0.0",
  "scripts": {
    "ng": "node --max_old_space_size=8050 ./node_modules/@angular/cli/bin/ng",
    "start": "node --max_old_space_size=8050 ./node_modules/@angular/cli/bin/ng serve --project=main-app",
    "build": "node --max_old_space_size=8050 ./node_modules/@angular/cli/bin/ng build --project=main-app",
    "vercel-build": "node --max_old_space_size=8050 ./node_modules/@angular/cli/bin/ng build --project=main-app",
    "watch": "node --max_old_space_size=8050 ./node_modules/@angular/cli/bin/ng build --watch --configuration development",
    "test": "node --max_old_space_size=8050 ./node_modules/@angular/cli/bin/ng test"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.0.0",
    "@angular/common": "^16.0.0",
    "@angular/compiler": "^16.0.0",
    "@angular/core": "^16.0.0",
    "@angular/forms": "^16.0.0",
    "@angular/platform-browser": "^16.0.0",
    "@angular/platform-browser-dynamic": "^16.0.0",
    "@angular/router": "^16.0.0",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.12.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.0.0",
    "@angular/cli": "^16.0.0",
    "@angular/compiler-cli": "^16.0.0",
    "@types/node": "^12.11.1",
    "@types/jasmine": "~3.10.0",
    "@types/jasminewd2": "~2.0.3",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.10.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.9.5"
  },
  "engines": {
    "node": "22.x"
  }
